package com.devsuperior.uri2621.dto;

import com.devsuperior.uri2621.projections.ProductMinProjection;

public class ProductDTO {
    private String name;

    public ProductDTO(){
    }

    public ProductDTO(String name) {
        this.name = name;
    }

    public  ProductDTO(ProductMinProjection entity){
        name=entity.getName();
    }
}
